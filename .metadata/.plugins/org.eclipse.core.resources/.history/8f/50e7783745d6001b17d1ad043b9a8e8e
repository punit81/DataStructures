package BinaryNode;

public class BNode {
	private int data;
	private BNode left;
	private BNode right;
	private boolean isEmpty;
	public BNode() {
		this.data=0;
		this.left=null;
		this.right=null;
		this.isEmpty=true;
	}
	public BNode(int data) {
		this.data=data;
		this.left=null;
		this.right=null;
		this.isEmpty=false;
	}
	public BNode(int data,BNode left,BNode right) {
		this.data=data;
		this.left=left;
		this.right=right;
		this.isEmpty=false;
	}
	public int getData() {
		return this.data;
	}
	public void setData(int data) {
		this.data=data;
		isEmpty=false;
	}
	public BNode getLeft() {
		return this.left;
	}
	public void setLeft(BNode left) {
		this.left=left;
	}
	public BNode getRight() {
		return this.right;
	}
	public void setRight(BNode right) {
		this.right=right;
	}
	public boolean IsEmpty() {
		return this.isEmpty;
	}
}